  "components": {
    "button": {
      "height": {
        "sm": "32px",
        "md": "40px",
        "lg": "48px"
      }
    },
    "card": {
      "padding": {
        "sm": "12px",
        "md": "24px",
        "lg": "36px"
      }
    },
    "input": {
      "height": "44px",
      "paddingX": "12px",
      "borderWidth": "1px"
    },
    "navigation": {
      "height": "64px",
      "sidebarWidth": "280px"
    }
  }
